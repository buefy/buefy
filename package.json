{
  "name": "@ntohq/buefy-next",
  "version": "0.1.1",
  "homepage": "https://buefy.org",
  "description": "Lightweight UI components for Vue.js (v3) based on Bulma",
  "author": "Rafael Beraldo <rafael.pimpa@gmail.com>",
  "maintainers": [
    "Walter Tommasi <tommsi20@gmail.com>",
    "Kikuo Emoto <kemoto@codemonger.io>",
    "Wesley Ford <wes@wesdevpro.com>"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/buefy.min.js",
  "typings": "types/index.d.ts",
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "src",
    "types/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ntohq/buefy-next"
  },
  "bugs": {
    "url": "https://github.com/ntohq/buefy-next/issues"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "lint": "eslint --ext .js,.vue src docs",
    "lint:fix": "eslint --ext .js,.vue src docs --fix",
    "lint:staged": "lint-staged",
    "unit": "jest --runInBand --u",
    "test": "npm run lint && npm run unit",
    "test:watch": "npm run unit -- --watchAll",
    "build:js": "rollup -c && rollup -c --environment MINIFY",
    "build:js:watch": "rollup -c --watch",
    "build:scss": "sass src/scss/buefy-build.scss | node ./build/banner.js > dist/buefy.css && cleancss -o dist/buefy.min.css dist/buefy.css",
    "build:scss:watch": "sass src/scss/buefy-build.scss --watch",
    "build:lib": "rimraf dist && npm run build:js && npm run build:scss && npm run vetur",
    "build:lib:watch": "concurrently \"npm run build:js:watch\" \"npm run build:scss:watch \" ",
    "build:docs": "node build/build.js && copyfiles --flat netlify.toml dist-docs",
    "build": "npm run build:lib && npm run build:docs",
    "cov": "./node_modules/codcov/bin/codcov",
    "vetur": "node -r esm build/vetur.js",
    "prepare": "npm run build:lib"
  },
  "keywords": [
    "bulma",
    "vue",
    "vuejs",
    "vue-bulma",
    "components"
  ],
  "dependencies": {
    "@babel/runtime": "7.18.9",
    "bulma": "0.9.4"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@applitools/eyes-cypress": "3.7.5",
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@fortawesome/fontawesome-free": "5.3.1",
    "@mdi/font": "4.8.95",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/test-utils": "^2.0.2",
    "all-contributors-cli": "6.14.0",
    "autoprefixer": "7.1.1",
    "axios": "0.21.1",
    "babel-jest": "24.9.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-runtime": "6.22.0",
    "babel-preset-env": "1.3.2",
    "babel-preset-stage-2": "6.22.0",
    "babel-register": "6.22.0",
    "bulma-css-vars": "^0.8.0",
    "chalk": "4.1.0",
    "clean-css-cli": "^5.6.1",
    "cleave.js": "1.0.1",
    "clipboard": "1.7.1",
    "codecov": "3.7.1",
    "connect-history-api-fallback": "1.3.0",
    "copy-webpack-plugin": "^6.4.1",
    "copyfiles": "2.2.0",
    "cross-env": "4.0.0",
    "cross-spawn": "5.0.1",
    "css-loader": "^5.2.7",
    "cypress": "3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-vue": "^9.2.0",
    "eslint-webpack-plugin": "^2.7.0",
    "esm": "3.2.25",
    "eventsource-polyfill": "0.9.6",
    "express": "4.15.0",
    "file-loader": "^6.2.0",
    "font-awesome": "4.7.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "handlebars": "4.5.3",
    "highlight.js": "10.4.1",
    "html-webpack-plugin": "^4.5.2",
    "http-proxy-middleware": "0.17.3",
    "jest": "24.9.0",
    "jest-serializer-vue": "^3.1.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.6.2",
    "open": "7.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "optimize-js-plugin": "0.0.4",
    "postcss": "^8.4.14",
    "pre-commit": "1.2.2",
    "prerender-spa-plugin": "^3.4.0",
    "raw-loader": "^4.0.2",
    "rimraf": "2.6.0",
    "rollup": "^2.77.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.53.0",
    "sass-loader": "^10.2.0",
    "scrollreveal": "3.3.6",
    "semver": "5.3.0",
    "shelljs": "0.8.4",
    "sortablejs": "1.7.0",
    "tiny-emitter": "^2.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "vee-validate": "2.1.0-beta.9",
    "vue": "^3.2.37",
    "vue-analytics": "4.1.3",
    "vue-breakpoint-mixin": "^1.2.2",
    "vue-eslint-parser": "^7.11.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "16.2.0",
    "vue-progressbar": "0.7.2",
    "vue-router": "^4.1.2",
    "vue-style-loader": "^4.1.3",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^3.11.3",
    "webpack-hot-middleware": "^2.25.1",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "*.{vue,js}": [
      "eslint --fix"
    ]
  },
  "pre-commit": "lint:staged",
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
